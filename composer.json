{
    "name": "webbuilders-group/silverstripe-site-template",
    "description": "Site's Description",
    "license": "proprietary",
    "require": {
        "php": ">=7.4.0",
        "silverstripe/recipe-cms": "4.10.*@stable",
        "wilr/silverstripe-googlesitemaps": "~2.1",
        "silverstripe/login-forms": "~4.0",
        "silverstripers/elemental-search": "dev-master",
        "webbuilders-group/silverstripe-next-gen-images": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "silverleague/ideannotator": "~3.0",
        "squizlabs/php_codesniffer": "~3.4"
    },
    "extra": {
        "resources-dir": "_resources",
        "project-files-installed": [
            "app/.htaccess",
            "app/_config.php",
            "app/_config/database.yml",
            "app/_config/mimevalidator.yml",
            "app/_config/mysite.yml",
            "app/src/Page.php",
            "app/src/PageController.php"
        ],
        "public-files-installed": [
            ".htaccess",
            "index.php",
            "install-frameworkmissing.html",
            "install.php",
            "web.config"
        ],
        "expose": [
            "themes/starter/css",
            "themes/starter/images",
            "themes/starter/javascript",
            "themes/starter/webfonts"
        ]
    },
    "config": {
        "process-timeout": 600
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
